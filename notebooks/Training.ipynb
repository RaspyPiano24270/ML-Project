      "metadata": {},
      "outputs": [],
      "source": [
        "import sys\n",
        "import os\n",
        "\n",
        "# Add the 'src' folder to Python's path so we can import our own scripts\n",
        "module_path = os.path.abspath(os.path.join('..', 'src'))\n",
        "if module_path not in sys.path:\n",
        "    sys.path.append(module_path)\n",
        "\n",
        "from data_loader import load_kaggle_csv\n",
        "print(\"Setup Complete. SRC folder is now accessible.\")"
      ]
    },
